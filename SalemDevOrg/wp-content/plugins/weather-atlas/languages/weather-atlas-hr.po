msgid "(on mobile devices current condition details are hidden)"
msgstr "(na mobilnim uređajima detalji o trenutnom stanju su skriveni)"

#: includes/class-weather-atlas.php:474
msgid "Hourly forecast"
msgstr "Satna prognoza"

#: includes/class-weather-atlas.php:482
msgid "hide hourly forecast"
msgstr "sakrij satnu prognozu"

#: includes/class-weather-atlas.php:483
msgid "hour"
msgstr "sat"

#: includes/class-weather-atlas.php:484 includes/class-weather-atlas.php:485
