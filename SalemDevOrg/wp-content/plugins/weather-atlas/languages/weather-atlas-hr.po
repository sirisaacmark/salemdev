msgid "(on mobile devices more then 3 days are hidden)"
msgstr "(na mobilnim uređajima sakriveno duže od 3 dana)"

#: includes/class-weather-atlas.php:528 includes/class-weather-atlas.php:539
msgid "Detailed forecast"
msgstr "Detaljna prognoza"

#: includes/class-weather-atlas.php:543
msgid "checked"
msgstr "označeno"

#: includes/class-weather-atlas.php:545
msgid "recommended"
msgstr "preporučljivo"

#: includes/class-weather-atlas.php:547
msgid ""
"removes \"<i>Weather from Weather Atlas</i>\" & places link \"<i>Detailed "
"forecast</i>\" for choosen location"
msgstr ""
"uklanja \"<i>Vrijeme od Weather Atlas</i>\" & stavlja poveznicu "
"\"<i>Detaljna prognoza </i>\" za odabranu lokaciju"

#: includes/class-weather-atlas.php:549
msgid "unchecked"
msgstr "neoznačeno"

#: includes/class-weather-atlas.php:551
msgid "shows \"<i>Weather from Weather Atlas</i>\" at the bottom of the widget"
msgstr "prikazuje \"<i>Vrijeme sa Weather Atlas</i>\" na dnu dodatka"

